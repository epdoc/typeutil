{
  "name": "epdoc-util",
  "version": "0.2.0",
  "description": "Typescript-aware guard tests and other standalone utilities",
  "private": false,
  "keywords": [
    "util",
    "typescript"
  ],
  "license": "MIT",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpravetz/epdoc-util.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "watch": "jest --watch",
    "test": "jest --runInBand",
    "coverage": "jest --coverage --runInBand --verbose",
    "prepublishOnly": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^5.2.2"
  }
}
