{
  "name": "@epdoc/typeutil",
  "version": "2.2.1",
  "description": "Typescript-aware guard tests and other standalone utilities",
  "private": false,
  "keywords": [
    "util",
    "typescript"
  ],
  "license": "MIT",
  "author": {
    "name": "James Pravetz",
    "email": "jpravetz@epdoc.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epdoc/typeutil.git"
  },
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "build": "rm -rf dist && tsc",
    "watch": "jest --watch",
    "clear-warning": "watchman watch-del-all > /dev/null",
    "test": "npm run clear-warning; jest --runInBand",
    "coverage": "jest --coverage --runInBand --verbose",
    "prepublishOnly": "tsc",
    "publish": "npm publish --access public",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  }
}
